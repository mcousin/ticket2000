<h1>Super Dashboard</h1>

<br />

<table class="table table-striped table-condensed">
  <tr>
    <th>Subject</th>
    <th>Status</th>
    <th>Description</th>
  </tr>	
	
<% @tickets.each do |ticket| %>
  <tr>
    <td><%= link_to ticket.subject, ticket %></td>
    <td><%= ticket.status %></td>
    <td><%= ticket.description %></td>
  </tr>
<% end %>
</table>
